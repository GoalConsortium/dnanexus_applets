{
  "name": "GATK",
  "title": "GATK",
  "summary": "GATK",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "Consensus_BAM",
      "label": "Consensus BAM (Uniq BAM)",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "ref_file",
      "label": "Reference Files tar.gz",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "pair_id",
      "label": "ReadGroup/SampleName",
      "class": "string",
      "optional": false,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "finalbam",
      "label": "BAM after GATK",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 24
      }
    },
    "interpreter": "bash",
    "release": "16.04",
    "distribution": "Ubuntu",
    "file": "src/GATK.sh"
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "azure:westus": {
      "systemRequirements": {
        "*": {
          "instanceType": "azure:mem1_ssd1_x16"
        }
      }
    }
  }
}
